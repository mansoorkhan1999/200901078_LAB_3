# 200901078_Task_1
import numpy as np
class queue:
 def __init__(self):
    
    self.list=np.arange(5)
 def enqueue(self,data):
      x=np.append(self.list,data)
      return x
 def dequeue(self):
    np.delete(self.list,0)
 def front(self):
    return self.list[0]
 def rear(self):
    return self.list[-1]
 def isempty(self):
    if (len(self.list)==0):
      return True
    else:
      return False
 def length(self):
    return len(self.list)
 def display(self):
    return self.list
